<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/revisionmanager/db" VERSION="20250625" COMMENT="XMLDB file for Moodle blocks/revisionmanager"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_revisionmanager_entries" COMMENT="Stores revisionmanager data per page and user">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="pageurl" TYPE="char" LENGTH="256" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="nextreview" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="pagetitle" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="learninglevel" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="revisioncount" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="targetcount" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
